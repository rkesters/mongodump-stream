{
  "name": "@rkesters/mongodump-stream",
  "version": "1.1.4",
  "description": "Various streaming utilities for dumping a mongo DB",
  "main": "index.js",
  "types": "index.d.td",
  "keywords": [
    "mongo",
    "fixture",
    "testing",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rkesters/mongodump-stream.git"
  },
  "homepage": "https://github.com/rkesters/mongodump-stream",
  "scripts": {
    "test": "npx jest --forceExit",
    "prebuild": "npx del-cli ./dist",
    "tsc": "npx tsc -b ./tsconfig-build.json ",
    "build": "npm run tsc",
    "postbuild": "npx copyfiles package.json README.md dist",
    "pub": "npm publish ./dist"
  },
  "author": "Robert Kesterson",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.636.0",
    "cosmiconfig": "^6.0.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.4",
    "q": "^1.1.1",
    "ramda": "^0.8.0",
    "stack-trace": "0.0.10"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/stack-trace": "0.0.29",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/mongodb": "^3.5.2",
    "@types/node": "^13.9.0",
    "@types/q": "^1.5.2",
    "copyfiles": "^2.2.0",
    "del-cli": "^3.0.0",
    "jest": "^25.1.0",
    "jest-environment-mongodb": "^1.0.2",
    "mongodb-memory-server-global": "^6.3.2",
    "prettier": "1.19.1",
    "ts-jest": "^25.2.1",
    "tslint": "5.17.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0",
    "tslint-misc-rules": "3.5.1",
    "typescript": "3.8.2",
    "request": "^2.48.0"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.2.2",
      "debug": "0",
      "disablePostinstall": "1",
      "md5Check": "1",
      "autostart": false
    }
  },
  "mongoDumpStream": {
    "chunkSize": 10
  }
}
